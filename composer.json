{
    "name": "sepiphy/laravel-support",
    "description": "The sepiphy/laravel-support package",
    "keywords": [
        "sepiphy", "laravel", "laravel-support"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/laratools",
    "support": {
        "issues": "https://github.com/sepiphy/laratools/issues",
        "source": "https://github.com/sepiphy/laratools"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "illuminate/contracts": "5.8.*",
        "illuminate/database": "5.8.*",
        "illuminate/http": "5.8.*",
        "illuminate/support": "5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Sepiphy\\Laravel\\Support\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Sepiphy\\Laravel\\Support\\SupportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
