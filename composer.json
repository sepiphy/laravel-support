{
    "name": "sepiphy/laravel-support",
    "description": "Shared Utilities for Laravel",
    "keywords": [
        "sepiphy", "laravel", "laravel-support"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/laravel-support",
    "support": {
        "issues": "https://github.com/sepiphy/laravel-support/issues",
        "source": "https://github.com/sepiphy/laravel-support"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "illuminate/contracts": "^5.8 | ^6.0 | ^7.0 | ^8.0",
        "illuminate/database": "^5.8 | ^6.0 | ^7.0 | ^8.0",
        "illuminate/http": "^5.8 | ^6.0 | ^7.0 | ^8.0",
        "illuminate/support": "^5.8 | ^6.0 | ^7.0 | ^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Sepiphy\\Laravel\\Support\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Sepiphy\\Laravel\\Support\\SupportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
