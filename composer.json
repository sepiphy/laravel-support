{
    "name": "sepiphy/laravel-support",
    "description": "The Sepiphy Laravel Support package.",
    "keywords": [
        "sepiphy", "laravel", "laravel-support"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/laravel-extensions",
    "support": {
        "issues": "https://github.com/sepiphy/laravel-extensions/issues",
        "source": "https://github.com/sepiphy/laravel-extensions"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com",
            "homepage": "https://seriquynh.com",
            "role": "Software Developer"
        }
    ],
    "require": {
        "illuminate/contracts": "5.8.*",
        "illuminate/database": "5.8.*",
        "illuminate/http": "5.8.*",
        "illuminate/support": "5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Sepiphy\\Laravel\\Support\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Sepiphy\\Laravel\\Support\\SupportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
